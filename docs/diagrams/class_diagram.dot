        labelloc=t
        labeljust=c
    ]
    
    node [
        fontname="Segoe UI"
        fontsize=10
        shape=none
        margin=0
    ]
    
    edge [
        fontname="Segoe UI"
        fontsize=9
        color="#475569"
    ]

